{
  "name": "homebridge-http-entry",
  "version": "1.1.0",
  "description": "Homebridge plugin that creates a GarageDoorOpener which uses configurable HTTP calls to set and get its state.",
  "repository": {
    "type": "git",
    "url": "git://github.com/nicinabox/homebridge-http-entry.git"
  },
  "bugs": {
    "url": "https://github.com/nicinabox/homebridge-http-entry/issues"
  },
  "main": "./dist/index.js",
  "engines": {
    "node": ">=14.18.1",
    "homebridge": ">=1.3.5"
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "got": "^11.1.4",
    "homebridge": "^1.3.0",
    "polling-to-event": "^2.0.2",
    "xmldom": "^0.6.0",
    "xpath": "^0.0.32"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.12",
    "@types/xmldom": "^0.1.29",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "babel-jest": "^27.4.5",
    "eslint": "^8.4.1",
    "http-server": "^14.0.0",
    "jest": "^27.4.5",
    "nock": "^13.2.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "scripts": {
    "test": "jest",
    "http-server": "http-server -p 8180 -e txt src/TestGateAccessory --proxy http://localhost:8180?",
    "lint": "eslint src/**.ts",
    "watch": "npm run build && npm link && nodemon",
    "build": "rimraf ./dist && tsc",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "keywords": [
    "homebridge-plugin",
    "homebridge",
    "homekit",
    "garagedoor",
    "gate"
